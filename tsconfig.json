{
  "compilerOptions": {
    "target": "ES6",
    "module": "ES6",
    "moduleResolution": "Bundler",
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "outDir": "build",
    "lib": [
      "es2016"
    ],
    "types": [
      "google-apps-script",
      "jest",
      "node"
    ]
  },
  "include": [
    "src",
  ]
}
